{
  "expo": {
    "name": "TabataTimer",
    "slug": "TabataTimer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/tt-logo-mobile-icon-2.png",
    "privacy": "unlisted",
    "splash": {
      "image": "./app/assets/tt-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tabatatimes.timer",
      "buildNumber": "1.0.1",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "bugsnag": {
        "apiKey": "b4b8f86e02be8490e581c7a45c86805b"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "@bugsnag/expo/hooks/post-publish.js",
          "config": {}
        }
      ]
    }
  }
}